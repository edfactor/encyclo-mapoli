{
    "Logging": {
        "LogLevel": {
            "Default": "Information",
            "Microsoft.Hosting.Lifetime": "Information",
            "Microsoft.EntityFrameworkCore.Database.Command": "Warning",
            "Microsoft.EntityFrameworkCore.Infrastructure": "Warning"
        }
    },
    "SmartLogging": {
        "ProjectName": "ProfitSharing",
        "Namespace": "Demoulas",
        "FileSystem": {
            "EnableFileSystemLogging": false,
            "EnableTextFileLogging": false,
            "EnableJsonFileLogging": false,
            "Path": "logs/log_.log",
            "JsonPath": "logs/log_structured_.log",
            "RetainedFileCountLimit": 7
        }
    }
}
