{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "SmartLogging": {
    "ProjectName": "ProfitSharing",
    "Namespace": "Demoulas",
    "FileSystem": {
      "EnableFileSystemLogging": false,
      "EnableTextFileLogging": false,
      "EnableJsonFileLogging": false,
      "Path": "logs/log_.log",
      "JsonPath": "logs/log_structured_.log",
      "RetainedFileCountLimit": 7
    }
  },
  "ProcessWatchdog": {
    "Enabled": true,
    "HeartbeatTimeoutSeconds": 300,
    "CheckIntervalSeconds": 30,
    "AlertOnMissedHeartbeats": 2
  }
}
