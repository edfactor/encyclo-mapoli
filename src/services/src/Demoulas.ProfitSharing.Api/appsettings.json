{
    "Logging": {
        "LogLevel": {
            "Default": "Warning",
            "Microsoft.AspNetCore": "Warning"
        }
    },
    "SmartLogging": {
        "ProjectName": "ProfitSharing",
        "Namespace": "Demoulas",
        "FileSystem": {
            "EnableFileSystemLogging": false,
            "EnableTextFileLogging": false,
            "EnableJsonFileLogging": false,
            "Path": "logs/log_.log",
            "JsonPath": "logs/log_structured_.log",
            "RetainedFileCountLimit": 7
        }
    },
    "AllowedHosts": "*",
    "LongRunningOperations": {
        "YearEndReportsTimeoutMinutes": 2,
        "MaxConcurrentReports": 5,
        "EnableProgressReporting": true
    },
    "Telemetry": {
        "Enabled": true,
        "EnableSensitiveFieldTracking": false,
        "LargeResponseThresholdBytes": 5000000,
        "IncludeUserRole": true,
        "DetailedTraceSampleRate": 0.1
    }
}
