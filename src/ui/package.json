    "test": "vitest --coverage",
    "test:windows": "vitest --coverage --run --pool=threads --poolOptions.threads.singleThread",
    "e2e": "playwright test --workers=2",
    "e2e:watch": "playwright test --watch",
    "e2e:ui": "playwright test --ui",
    "tsc": "npx tsc -b --noEmit",
