{
  "openapi": "3.0.0",
  "info": {
    "title": "Profit Sharing API",
    "version": "1.0.0",
    "description": "Profit sharing management system API"
  },
  "servers": [
    {
      "url": "https://api.example.com",
      "description": "Production"
    }
  ],
  "tags": [
    {
      "name": "YearEnd",
      "description": "Year-end processing and reports"
    },
    {
      "name": "Reports",
      "description": "Reporting operations"
    },
    {
      "name": "Beneficiaries",
      "description": "Beneficiary management"
    }
  ],
  "paths": {
    "/enrollments": {
      "post": {
        "tags": ["YearEnd"],
        "summary": "Update enrollments",
        "description": "Updates the enrollment ID of all members for a given profit year. Accepts profit year as optional route parameter or in request body.",
        "operationId": "updateEnrollments",
        "requestBody": {
          "required": true,
          "content": {
            "application/json": {
              "schema": {
                "type": "object",
                "properties": {
                  "profitYear": {
                    "type": "integer"
                  }
                }
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "Success"
          },
          "400": {
            "description": "Invalid request"
          }
        }
      }
    },
    "/beneficiaries": {
      "put": {
        "tags": ["Beneficiaries"],
        "summary": "Update beneficiary",
        "description": "Updates an existing beneficiary's information. Requires beneficiary ID in request body.",
        "operationId": "updateBeneficiary",
        "requestBody": {
          "required": true,
          "content": {
            "application/json": {
              "schema": {
                "type": "object"
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "Success"
          },
          "404": {
            "description": "Beneficiary not found"
          }
        }
      }
    },
    "/stores/breakdown": {
      "get": {
        "tags": ["Reports"],
        "summary": "Get store breakdown",
        "description": "Retrieves a breakdown of managers and associates by store for all stores in the profit sharing program. Returns member summary data organized by store.",
        "operationId": "getBreakdown",
        "parameters": [
          {
            "name": "profitYear",
            "in": "query",
            "required": false,
            "schema": {
              "type": "integer"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "Success"
          }
        }
      }
    },
    "/stores/{storeNumber}/breakdown/totals": {
      "get": {
        "tags": ["Reports"],
        "summary": "Get store breakdown totals",
        "description": "Retrieves total breakdown data for managers and associates at a specific store. Requires QPAY066TA authorization.",
        "operationId": "getBreakdownTotals",
        "parameters": [
          {
            "name": "storeNumber",
            "in": "path",
            "required": true,
            "schema": {
              "type": "string"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "Success"
          }
        }
      }
    },
    "/stores/breakdown/totals": {
      "get": {
        "tags": ["Reports"],
        "summary": "Get grand total breakdown",
        "description": "Retrieves grand total breakdown data for managers and associates across all stores. Results are cached for performance.",
        "operationId": "getGrandTotals",
        "parameters": [
          {
            "name": "profitYear",
            "in": "query",
            "required": false,
            "schema": {
              "type": "integer"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "Success"
          }
        }
      }
    },
    "/executive-hours": {
      "put": {
        "tags": ["YearEnd"],
        "summary": "Update executive hours",
        "description": "Updates executive hours and dollars allocations for a profit year. Requires FINANCEMANAGER role.",
        "operationId": "setExecutiveHours",
        "requestBody": {
          "required": true,
          "content": {
            "application/json": {
              "schema": {
                "type": "object"
              }
            }
          }
        },
        "responses": {
          "204": {
            "description": "Updated"
          }
        }
      }
    },
    "/divorce-report": {
      "post": {
        "tags": ["Reports"],
        "summary": "Account History Report",
        "description": "Returns a report of member account activity condensed by profit year. Includes contributions, earnings, forfeitures, withdrawals, and ending balances for each plan year.",
        "operationId": "getAccountHistory",
        "requestBody": {
          "required": true,
          "content": {
            "application/json": {
              "schema": {
                "type": "object"
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "Success"
          }
        }
      }
    },
    "/profit-share-edit": {
      "get": {
        "tags": ["Reports"],
        "summary": "Profit Share Edit",
        "description": "Returns per member transactions based on user specified contribution/incoming forfeit/earnings parameters",
        "operationId": "getProfitShareEdit",
        "parameters": [
          {
            "name": "profitYear",
            "in": "query",
            "required": true,
            "schema": {
              "type": "integer"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "Success"
          }
        }
      }
    },
    "/beneficiaries-report": {
      "get": {
        "tags": ["Beneficiaries"],
        "summary": "Adhoc Beneficiaries Report",
        "description": "Returns a report of employee and non-employee beneficiaries, with optional detail lines.",
        "operationId": "getAdhocBeneficiaries",
        "parameters": [
          {
            "name": "includeDetail",
            "in": "query",
            "required": false,
            "schema": {
              "type": "boolean"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "Success"
          }
        }
      }
    },
    "/profit-sharing-update": {
      "get": {
        "tags": ["Reports"],
        "summary": "Profit Sharing Update",
        "description": "Updates plan members based on points provided.",
        "operationId": "getProfitSharingUpdate",
        "parameters": [
          {
            "name": "profitYear",
            "in": "query",
            "required": true,
            "schema": {
              "type": "integer"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "Success"
          }
        }
      }
    },
    "/frozen/distributions-by-age": {
      "get": {
        "tags": ["Reports"],
        "summary": "Distributions by age",
        "description": "This report produces a list of members showing their distribution over the year grouped by age and aggregated to separate regular from hardships",
        "operationId": "getDistributionsByAge",
        "parameters": [
          {
            "name": "profitYear",
            "in": "query",
            "required": true,
            "schema": {
              "type": "integer"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "Success"
          }
        }
      }
    },
    "/frozen/balance-by-age": {
      "get": {
        "tags": ["Reports"],
        "summary": "Balance by age",
        "description": "This report produces a list of members showing their balances over the year grouped by age",
        "operationId": "getBalanceByAge",
        "parameters": [
          {
            "name": "profitYear",
            "in": "query",
            "required": true,
            "schema": {
              "type": "integer"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "Success"
          }
        }
      }
    }
  }
}
